<div class="row">

    {{#each uploads}}
     
   
  
    <div class="col-md-3">
        <div class="card">
            <div class="card-body ">
                <h4 class="card-title d-flex justify-content-between align-items-center">titulo:{{name}}</h4>
                  <!-- PUT REQUEST NOT YET IMPLEMEMNTET-->
                  {{#if user}}
                  
                      <button href={{first_alt}}>First</button> <br>
                      <a>{{{second_alt}}}</a> <br>
                      <a>{{third_alt}}</a> <br>
                      <a>{{fourth_alt}}</a> <br>
                      <a href="{{fifth_alt}}">{{fifth_alt}}</a> <br>
                      
                 <!-- DELETE REQUEST NOT YET IMPLEMENTET-->
                  <form action="/app/app/games/games/delete/{{_id}}?_method=DELETE" method="POST">
                    <input type="hidden" name="_method" value="DELETE">
                    <button type="submit" class="btn btn-danger btn-block btn-sm">Delete</button>
                  </form>
                  {{else}}
                    
                  <h4 class="card-title d-flex justify-content-between align-items-center">{{name}}
                    <a href="/app/app/games/games/edit/{{_id}}"><i class="fas fa-edit"></i></a></h4>
                      
                      <a href="{{first_alt}}">Primera alternativa</a> <br>
                      <a href="{{second_alt}}">Segunda alternativa</a> <br>
                    <a href="{{third_alt}}">Tercera alternativa</a> <br>
                      <a href="{{fourth_alt}}">Cuarta alternativa</a> <br>
                    <a href="{{fifth_alt}}">Quinta alternativa</a> <br>
                  <p>{{#if this.classy}}First{{/if}}</p>
                  <p>{{#if this.first}} seconnnndd {{/if}}</p>
                      </div>
                      
                  </div>
                    
              </div>
              {{/if}}
              
     {{else}}
  <div class="card mx-auto">
    <div class="card-body">
      <h1>Hello {{user.name}}</h1>
      <p clsss="lead">There are no pdfs yet.</p>
      <a href="/app/app/games/games/add" class="btn btn-success btn-block">Create One!</a>
    </div>
  </div>
 
  {{/each}}
 
</div>